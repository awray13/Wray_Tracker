@section scripts {
    <script>
        var unAuthorizedTicketAccess = '@TempData["UnAuthorizedTicketAccess"]';
        if (unAuthorizedTicketAccess != "") {
            Swal.fire({
                title: 'Unauthorized Ticket Access Detected!',
                text: unAuthorizedTicketAccess,
                icon: 'error',
                confirmButtonText: 'OK'
            })
        }
    </script>
