                            <p class="text-sm mx-sm-4">All Projects in the Database</p>
                        </div>
                        <!-- /.card-header -->
                        <div class="card-body">
                            <table class="table table-valign-middle border-bottom">
                                <thead class="table-head-fixed">
                                    <tr>
                                        <th>
                                            @Html.DisplayName("Project Name")
                                        </th>
                                        <th>
                                            @Html.DisplayName("Description")
                                        </th>
                                        <th></th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var project in Model.ProjectVM.AllProjects)
                                    {
                                        <tr>
                                            <td>
                                                @Html.DisplayFor(modelItem => project.Name)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => project.Description)
                                            </td>
                                            <td>
                                                <nav>
                                                    <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
                                                        <li class=" nav-item has-treeview">
                                                            <a href="#" class="nav-link">
                                                                <p>
                                                                    More Options
                                                                    <i class="fas fa-angle-down"></i>
                                                                </p>
                                                            </a>
                                                            <ul class="nav nav-treeview">
                                                                <li class="nav-item">
                                                                    @Html.ActionLink("Assign Users", "AssignUsers", "Projects", new { projectid = project.Id }, new { @class = "nav-link" })
                                                                </li>
                                                                <li class="nav-item">
                                                                    @Html.ActionLink("Edit", "Edit", "Projects", new { id = project.Id }, new { @class = "nav-link" })
                                                                </li>
                                                                <li class="nav-item">
                                                                    @Html.ActionLink("Project Details", "ProjectDetails", "Projects", new { id = project.Id }, new { @class = "nav-link" })
                                                                </li>
                                                            </ul>
                                                        </li>
                                                    </ul>
                                                </nav>
                                            </td>
                                        </tr>
                                    }
                                </tbody>
                            </table>
                            <!-- ./card-body -->
                        </div>
                        <!-- /.card -->
                    </div>
                    <!-- /.col -->
                </div>
                <!-- /.row -->
            </div>
            <!-- /.row -->
            <!-- Ticket Table Row -->
            <div class="row">
                <div class="col">
                    <div class="card">
                        <div class="mx-lg-2 mt-lg-n2 elevation-2 bg-gradient-navy text-white">
                            <h5 class="mx-3 mt-2 mb-0">All Tickets</h5>
                            <p class="text-sm mx-sm-4">All Tickets in the Database</p>
                        </div>
                        <!-- /.card-header -->
                        <div class="card-body">
                            <table class="table table-valign-middle border-bottom">
                                <thead class="table-head-fixed">
                                    <tr>
                                        <th>
                                            @Html.DisplayName("Title")
                                        </th>
                                        <th>
                                            @Html.DisplayName("Description")
                                        </th>
                                        <th></th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var ticket in Model.AllTickets)
                                    {
                                        <tr>
                                            <td>
                                                @Html.DisplayFor(modelItem => ticket.Title)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => ticket.Description)
                                            </td>
                                            <td>
                                                <nav>
                                                    <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
                                                        <li class=" nav-item has-treeview">
                                                            <a href="#" class="nav-link">
                                                                <p>
                                                                    More Options
                                                                    <i class="fas fa-angle-down"></i>
                                                                </p>
                                                            </a>
                                                            <ul class="nav nav-treeview">
                                                                <li class="nav-item">
                                                                    @Html.ActionLink("Edit", "Edit", "Tickets", new { id = ticket.Id }, new { @class = "nav-link" })
                                                                </li>
                                                                <li class="nav-item">
                                                                    @Html.ActionLink("Ticket Dashboard", "Dashboard", "Tickets", new { id = ticket.Id }, new { @class = "nav-link" })
                                                                </li>
                                                            </ul>
                                                        </li>
                                                    </ul>
                                                </nav>
                                            </td>
                                        </tr>
                                    }
                                </tbody>
                            </table>
                            <!-- ./card-body -->
                        </div>
                        <!-- /.card -->
                    </div>
                    <!-- /.col -->
                </div>
                <!-- /.row -->
            </div>
            <!-- /.row -->
        </div>
        <!--/. container-fluid -->
    </section>
    <!-- /.content -->
}

